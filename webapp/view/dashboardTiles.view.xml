<mvc:View controllerName="com.limscloud.app.controller.dashboardTiles" displayBlock="true" height="100%" xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">
	<App id="dashboardTilesApp">
		<pages>
			<Page id="page2" showHeader="false" class="sapUiSizeCompact">
				<content>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create New System User" subheader="Singe creation" mode="HeaderMode"
						press="_navSU01">
						<TileContent footer="SU01">
							<ImageContent src="sap-icon://home-share"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Customer" subheader="Single creation" press="_navCU01">
						<TileContent footer="CU01">
							<ImageContent src="sap-icon://home-share"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Maintain Customers" subheader="Mass View/Modify" press="_navCU05">
						<TileContent footer="CU05">
							<ImageContent src="sap-icon://customer"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Receive Materials from Customer" subheader="Mass Transfer"
						mode="HeaderMode" press="_navMM01">
						<TileContent footer="MM01"/>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Task Unassigned" subheader="Unitary task" mode="HeaderMode"
						press="_navTT01">
						<TileContent footer="TT01">
							<ImageContent src="sap-icon://home-share"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Task for User" subheader="Unitary task" mode="HeaderMode"
						press="_navTT02">
						<TileContent footer="TT02">
							<ImageContent src="sap-icon://home-share"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Assign Task to User" subheader="Unitary task" mode="HeaderMode"
						press="_navTT03">
						<TileContent footer="TT03">
							<ImageContent src="sap-icon://meeting-room"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Launch Task" subheader="Tasks for user" 
					press="_navTL01">
						<TileContent footer="TL01">
							<ImageContent src="sap-icon://physical-activity"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Monitor Tasks" subheader="All tasks"
					press="_navTM01">
						<TileContent footer="TM01">
							<ImageContent src="sap-icon://electrocardiogram"/>
						</TileContent>
					</GenericTile>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>