<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog 
		id="test_disp_dialog"
		title="Test Discipline"
		class="sapUiPopupWithPadding"
		items="{jobsModel>/lookups/testDisp}"
		search="_hdlTestDispSearch"
		confirm="_hdlTestDispConfirm"
		cancel="_hdlTestDispCancel"
        noDataText="No disciplines found"
		>       
		<StandardListItem
			title="{jobsModel>disp_id}"
            info="{jobsModel>disp_name}"
			description="{jobsModel>disp_name}" />
	</SelectDialog>
</core:FragmentDefinition>