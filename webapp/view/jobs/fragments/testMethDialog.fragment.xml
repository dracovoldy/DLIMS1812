<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog 
		id="test_method_dialog"
		title="Test Method"
		class="sapUiPopupWithPadding"
		items="{jobsModel>/lookups/testMethods}"
		search="_handleInputSearch"
		confirm="_handleInputConfirm"
		cancel="_handleInputClose"
		>       
		<StandardListItem
			title="{jobsModel>MethodValue}"
            info="{jobsModel>MethodId}"
			description="{jobsModel>MethodDesc}" />
	</SelectDialog>
</core:FragmentDefinition>