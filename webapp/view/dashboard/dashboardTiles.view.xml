<mvc:View controllerName="com.limscloud.app.controller.dashboard.dashboardTiles" displayBlock="true" height="100%" 
	xmlns="sap.m" 
	xmlns:core="sap.ui.core" 
	xmlns:layout="sap.ui.layout" 
	xmlns:mvc="sap.ui.core.mvc">
	<App id="dashboardTilesApp">
		<pages>
			<Page id="page2" showHeader="false" class="sapUiSizeCompact">
				<content>

					<layout:VerticalLayout class="sapUiResponsiveMargin">
						<Title titleStyle="H2" text="Sales" class="sapUiTinyMarginBegin"/>
						<layout:HorizontalLayout allowWrapping="true">
							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Customer" subheader="Single creation" press="_navCU01">
								<TileContent footer="CU01">
									<ImageContent src="sap-icon://home-share"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Order" subheader="Single creation" press="_navOR01">
								<TileContent footer="OR01">
									<ImageContent src="sap-icon://customer"/>
								</TileContent>
							</GenericTile>


						</layout:HorizontalLayout>

						<Title titleStyle="H2" text="Materials Management" class="sapUiTinyMarginBegin sapUiMediumMarginTop"/>
						<layout:HorizontalLayout allowWrapping="true">

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Material" subheader="Stock" press="_navMM01">
								<TileContent footer="MM01">
									<ImageContent src="sap-icon://dimension"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Receive Sample" subheader="Stock Transfer" press="_navMM01">
								<TileContent footer="MM02">
									<ImageContent src="sap-icon://dimension"/>
								</TileContent>
							</GenericTile>


						</layout:HorizontalLayout>

						<Title titleStyle="H2" text="Review" class="sapUiTinyMarginBegin sapUiMediumMarginTop"/>
						<layout:HorizontalLayout allowWrapping="true">

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="New Review Document" subheader="Process" press="_navRD01">
								<TileContent footer="RD01">
									<ImageContent src="sap-icon://activity-items"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Edit Review Document" subheader="Process" press="_navRD02">
								<TileContent footer="RD02">
									<ImageContent src="sap-icon://activity-items"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Assign Reviews" subheader="Process" press="_navAD01">
								<TileContent footer="AD01">
									<ImageContent src="sap-icon://activity-items"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Approve Reviews" subheader="Process" press="_navRD50">
								<TileContent footer="RD50">
									<ImageContent src="sap-icon://activity-items"/>
								</TileContent>
							</GenericTile>

						</layout:HorizontalLayout>

						<Title titleStyle="H2" text="Testing" class="sapUiTinyMarginBegin sapUiMediumMarginTop"/>
						<layout:HorizontalLayout allowWrapping="true">

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Job" subheader="Process" press="_navJB01">
								<TileContent footer="JB01">
									<ImageContent src="sap-icon://activity-items"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Assign Job" subheader="Process" press="_navTT02">
								<TileContent footer="AD02">
									<ImageContent src="sap-icon://activity-individual"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Perform Job" subheader="Process" press="_navTT02">
								<TileContent footer="JB03">
									<ImageContent src="sap-icon://activity-individual"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Edit Job" subheader="Process" press="_navTT02">
								<TileContent footer="JB02">
									<ImageContent src="sap-icon://activity-individual"/>
								</TileContent>
							</GenericTile>

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Job Report" subheader="All tasks" press="_navTM01">
								<TileContent footer="TM01">
									<ImageContent src="sap-icon://electrocardiogram"/>
								</TileContent>
							</GenericTile>
						</layout:HorizontalLayout>

					</layout:VerticalLayout>



				</content>
			</Page>
		</pages>
	</App>
</mvc:View>